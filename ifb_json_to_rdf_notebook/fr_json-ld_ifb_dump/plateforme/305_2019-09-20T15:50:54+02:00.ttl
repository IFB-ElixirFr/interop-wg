    "http://purl.org/dc/terms/description": [
      {
        "@value": "<p>Programme Hétérogénéité Tumorale et Ecosystème – INSERM<br />\r\nIntegrating the intra-tumoral microbiome to promote understanding on tumor plasticity.<br />\r\n8 partenaires&nbsp;(4 Institut Pasteur units and 4 inserm units)<br />\r\nfunded by INSERM</p>\r\n"
      }
    ],
